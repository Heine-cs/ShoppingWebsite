@model IEnumerable<OllieShop.Models.Accounts>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Account)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Password)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Level)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UR)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Account)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Password)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Level)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UR.URID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ACID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ACID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ACID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
